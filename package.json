{
  "name": "slime-wrapper",
  "version": "0.4.0",
  "description": "Wrapper class for Typescript",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config=jest.config.js",
    "lint": "eslint --ext .js,.ts src",
    "build:types": "npx tsc --emitDeclarationOnly --project tsconfig.types.json",
    "build:docs": "rimraf ./docs && typedoc --plugin typedoc-plugin-markdown --out docs --includeVersion --excludeNotDocumented src/index.ts",
    "build": "rimraf ./lib && rollup --config rollup.config.ts --configPlugin typescript"
  },
  "author": "foreverAloneT",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.0",
    "rollup-plugin-dts": "^5.1.0",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.23",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  },
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ]
}
